// iex(1)> Enum.map([1, 2, 3], fn x -> x * 2 end)
// [2, 4, 6]
// iex(2)> Enum.sum([1, 2, 3])
// 6
// iex(3)> Enum.sum([2, 3, 11])
// 16
// iex(4)> Enum.map([3, 4, 10], fn x -> x * x end)
// [9, 16, 100]
// iex(5)> Enum.map(1..3, fn x -> x * 3 end)
// [3, 6, 9]
// iex(6)> map = %{"a" => 1, "b" => 2}
// %{"a" => 1, "b" => 2}
// iex(7)> Enum.map(map, fn{k, v} -> {k, v * 2} end)
// [{"a", 2}, {"b", 4}]